---
layout: layouts/main.njk
eleventyComputed:
  metaDescription: "{{ abstract }}"
---
<style>
  {% include "../../css/syntax_highlighting.css" %}
</style>

<section class="devider">
  <div class="content">
    <h1>{{ title }}</h1>
    <p>{{ abstract }}</p>
  </div>
</section>
<main>
  <div class="content">
    <article>
      {{ content | safe }}
    </article>
  </div>
</main>
